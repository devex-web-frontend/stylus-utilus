@import "stylus-utilus/mixins/positions.styl";

.test-positions-1 {
	fixed: top;
}
.test-positions-2 {
	fixed: top left;
}
.test-positions-3 {
	absolute: top 100px left right 7px;
}
.test-positions-4 {
	absolute: top 100px left right 7px bottom auto;
}
.test-positions-5 {
	relative: top 10px;
}
.test-positions-6 {
	relative: top 10px right;
}
.test-positions-7 {
	relative: top 10px z-index 10;
}
.test-positions-8 {
	absolute: bottom z 1 right 90%;
}